{
  "css.customData": ["./vscode.tailwind.json"],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro", // Enable .astro
    "typescript", // Enable .ts
    "typescriptreact" // Enable .tsx
  ],
  "files.associations": {
    "*.mdx": "markdown"
  },
  "prettier.documentSelectors": ["**/*.astro"],
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "yaml.schemas": {
    "./.vscode/astrowind/config-schema.json": "/src/config.yaml"
  },
  "workbench.colorCustomizations": {
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#fbfc2f",
    "statusBar.background": "#f3f404",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#c1c203",
    "statusBarItem.remoteBackground": "#f3f404",
    "statusBarItem.remoteForeground": "#15202b",
    "tab.activeBorder": "#fbfc2f",
    "titleBar.activeBackground": "#f3f404",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#f3f40499",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#f3f404"
}
