---
import logoImg from '~/assets/images/logo1.webp';
import { SITE_CONFIG } from '~/utils/config';
---

<span
  class="self-center ml-2 rtl:ml-0 rtl:mr-2 text-2xl md:text-xl font-bold text-gray-900 whitespace-nowrap dark:text-white"
>
  <img class="h-10 -mt-2 object-contain inline" src={logoImg.src} />
  {SITE_CONFIG?.name}
</span>
